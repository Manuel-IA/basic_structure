<div class="row">
  <div class="col s12 m6">
    <div class="card" id="<%= dom_id assignament %>" style="margin-bottom: 0px;">
      <div class="card-content">
        <span class="card-title"><%= assignament.owner_name %></span>
        <p>
          <strong>Owner name:</strong>
          <%= assignament.owner_name %>
        </p>

        <p>
          <strong>Phone number:</strong>
          <%= assignament.phone_number %>
        </p>

        <p>
          <strong>Email:</strong>
          <%= assignament.email %>
        </p>

        <p>
          <strong>Address:</strong>
          <%= assignament.address %>
        </p>

        <p>
          <strong>Vin number:</strong>
          <%= assignament.vin_number %>
        </p>

        <p>
          <strong>Po:</strong>
          <%= assignament.po %>
        </p>

        <p>
          <strong>Plate:</strong>
          <%= assignament.plate %>
        </p>

        <p>
          <strong>Deductible:</strong>
          <%= assignament.deductible %>
        </p>

        <p>
          <strong>Insurance aproval:</strong>
          <%= assignament.insurance_aproval %>
        </p>

        <p>
          <strong>Color:</strong>
          <%= assignament.color %>
        </p>

        <p>
          <strong>Status:</strong>
          <%= assignament.status[:name] %>
        </p>

        <p>
          <strong>Make:</strong>
          <%= assignament.make %>
        </p>

        <p>
          <strong>Model:</strong>
          <%= assignament.model %>
        </p>

        <p>
          <strong>Year:</strong>
          <%= assignament.year %>
        </p>

        <p>
          <strong>Comment:</strong>
          <%= assignament.comment %>
        </p>
      </div>
    </div>
  </div>
</div>
