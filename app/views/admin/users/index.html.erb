<div id="admin_users">
  <% @admin_users.each do |admin_user| %>
    <%= render admin_user %>
    <p>
      <%= link_to "Show this user", admin_user %> |
      <%= link_to "Edit this user", edit_admin_user_path(admin_user) %> |
      <%= link_to "Destroy this user", admin_user, data: { turbo_method: :delete, turbo_confirm: 'Are you sure ?' } %>
    </p>
  <% end %>
</div>

